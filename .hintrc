{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "scrollbar-width"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not ios_saf <= 16.5",
    "not opera <= 100",
    "not safari <= 16.5",
    "not samsung <= 21",
    "not and_ff <= 113",
    "not firefox <= 114"
  ]
}